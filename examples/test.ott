let Eq (A : U) (a₀ : A) (a₁ : A) : U
  = [ A @ nd { y } (nd (lf { x }) (lf tt)) 
        | lf { p }
        ] a₀ a₁

let refl (A : U) (a : A) : Eq A a a
  = [ a @ nd { y } (nd (lf { x }) (lf tt)) 
        | lf { p }
        ]

let PathOver (A : U) (B : A → U)
    (a₀ : A) (a₁ : A) (p : Eq A a₀ a₁)
    (b₀ : B a₀) (b₁ : B a₁) : U
  = [ B @ nd { y } (nd (lf { x }) (lf tt)) 
        | lf { p }
        ] a₀ a₁ p b₀ b₁

