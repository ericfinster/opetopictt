let Eq (A : U) (a₀ : A) (a₁ : A) : U
  = fst [ A
        @ nd { y } (nd (lf { x }) (lf tt)) 
        | lf { p }
        ] a₀ a₁

let refl (A : U) (a : A) : Eq A a a
  = [ a
    @ nd { y } (nd (lf { x }) (lf tt)) 
    | lf { p }
    ]

let PathOver (A : U) (B : A → U)
    (a₀ : A) (a₁ : A) (p : Eq A a₀ a₁)
    (b₀ : B a₀) (b₁ : B a₁) : U
  = fst ([ B
         @ nd { y } (nd (lf { x }) (lf tt)) 
         | lf { p }
         ] a₀ a₁ p) b₀ b₁

let transport (A : U) (B : A → U)
    (a₀ : A) (a₁ : A) (p : Eq A a₀ a₁)
    (b₀ : B a₀) : B a₁
  = fst snd ([ B
             @ nd { y } (nd (lf { x }) (lf tt)) 
             | lf { p }
             ] a₀ a₁ p) b₀

let Σ-eq (A : U) (B : A → U)
    (a₀ : A) (a₁ : A) (p : Eq A a₀ a₁)
    (b₀ : B a₀) (b₁ : B a₁) (q : PathOver A B a₀ a₁ p b₀ b₁)
  : Eq ((a : A) × B a) (a₀ , b₀) (a₁ , b₁)
  = p , q 

# ucomps are with us ...

let ucomp (A : U) (a : A) : A
  = fst snd [ A
            @ nd { y } (nd (lf { x }) (lf tt)) 
            | lf { p }
            ] a

let ufill (A : U) (a : A) : Eq A a (ucomp A a)
  = fst snd snd [ A
                @ nd { y } (nd (lf { x }) (lf tt)) 
                | lf { p }
                ] a

