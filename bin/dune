(ocamllex
  (modules lexer))

(menhir
  (infer false)
  (flags --table)
  (modules parser))

(rule
 (targets parser_messages.ml)
 (deps parser.messages parser.mly)
 (action  (with-stdout-to %{targets} (run menhir --compile-errors %{deps}))))

(executable
 (name erictt)
 (public_name erictt)
 (libraries base fmt menhirLib))
